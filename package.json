{
  "name": "stan-dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-parcel": "parcel ./src/index.html --open",
    "build": "parcel build ./src/index.html --public-url ./ --no-source-maps",
    "build-server": "babel ./server --out-dir dist",
    "build-watch": "parcel watch ./src/index.html --no-autoinstall",
    "start-watch": "nodemon  --exec babel-node ./server/index.js",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-watch\"",
    "debug": "nodemon --inspect --exec babel-node ./server/index.js",
    "start": "npm run build && node ./server/index.js",
    "serve": "node ./dist/index.js",
    "test:eslint": "eslint src/ --ext .js --ext .jsx",
    "pretest": "yarn run test:eslint",
    "precommit": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "@babel/runtime-corejs2": "^7.8.7",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "axios": "^1.6.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "clsx": "^1.0.4",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.27",
    "node-nats-streaming": "^0.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.12.0",
    "react-json-view": "^1.19.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-timeago": "^4.4.0",
    "react-transition-group": "^4.3.0",
    "recharts": "^2.0.0-beta.1",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "shortid": "^2.2.15",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@types/react": "^16.9.17",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,scss,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
